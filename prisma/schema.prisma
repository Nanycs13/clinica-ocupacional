generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "sqlite"
}

model ExameMedico {
  id_exame           Int      @id @default(autoincrement()) @map("id_exame")
  empresa            String   @map("empresa")
  medico_responsavel String   @map("medico_responsavel")
  data_exame         DateTime @map("data_exame")
  tipo_exame         String   @map("tipo_exame")
  resultado          String   @map("resultado")
  afastamento        String   @map("afastamento")

  @@map("exame_medico")
}

